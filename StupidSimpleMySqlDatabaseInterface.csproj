<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>StupidSimpleMySqlDatabaseInterface</Title>
        <Authors>Martiiin</Authors>
        <Description>This package simplifies connections and db operations with MySql!</Description>
        <PackageProjectUrl>https://github.com/MartinPrograms/StupidSimpleMySqlDatabaseInterface</PackageProjectUrl>
        <RepositoryUrl>https://github.com/MartinPrograms/StupidSimpleMySqlDatabaseInterface.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>db mysql database</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MySql.Data" Version="9.1.0" />
    </ItemGroup>

</Project>
